<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Image</th>
      <th>Content</th>
      <th>Status</th>
      <th>Node</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.image %></td>
        <td><%= article.content %></td>
        <td><%= article.status %></td>
        <td><%= article.node %></td>
        <td><%= article.order %></td>
        <td><%= link_to 'Show', article %></td>    
      </tr>
    <% end %>
  </tbody>
</table>

<br>

 
